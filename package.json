    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.1.1",
    "three": "^0.147.0",
    "tweakpane": "^3.0.7"
  },
  "devDependencies": {
    "@pablo-mayrgundter/testing.js": "^1.0.0",
    "@tsconfig/recommended": "^1.0.1",
