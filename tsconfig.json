{
  "extends": "@tsconfig/recommended/tsconfig.json",
  "include": [
    "./js/utils.js",
    "./js/shared.js",
    "./js/StarsCatalog.js",
    "index.d.ts",
  ],
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "jsx": "react-jsx",
    "lib": ["es6", "dom"],
    "module": "es6",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "pretty": false,
    "strict": true,
    "strictNullChecks": true,
  },
}
